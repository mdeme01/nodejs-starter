{
  "name": "nodejs-starter",
  "version": "1.0.0",
  "main": "src/index.ts",
  "scripts": {
    "prisma:migrate": "prisma migrate dev --schema ./src/prisma/schema.prisma --name",
    "prisma:generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node --esm' src/index.ts",
    "lint": "eslint",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cookie-parser": "^1.4.5",
    "@types/express": "^4.17.18",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.6.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2"
  }
}
